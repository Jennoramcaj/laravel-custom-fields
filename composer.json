{
    "name": "jennoramcaj/laravel-custom-fields",
    "description": "This is my package laravel-custom-fields",
    "keywords": [
        "Jennoramcaj",
        "laravel",
        "laravel-custom-fields"
    ],
    "homepage": "https://github.com/jennoramcaj/laravel-custom-fields",
    "license": "MIT",
    "authors": [
        {
            "name": "Jenno Ramcaj",
            "email": "jenno@ramcaj.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Jennoramcaj\\LaravelCustomFields\\": "src",
            "Jennoramcaj\\LaravelCustomFields\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jennoramcaj\\LaravelCustomFields\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jennoramcaj\\LaravelCustomFields\\LaravelCustomFieldsServiceProvider"
            ],
            "aliases": {
                "LaravelCustomFields": "Jennoramcaj\\LaravelCustomFields\\Facades\\LaravelCustomFields"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
